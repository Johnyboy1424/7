package testStuff;

public class SwitchStatements 
{
	public static void main(String args[])
	{
		String[] s = new String[4];
		int x = 1;
		s[0] = "f";
		s[1] = "r";
		switch(s[x])
		{
		case "f":
			System.out.println("LLL");
			break;
		case "r":
			System.out.println("SfSD");
			break;
		case
		
		}
		
	}
}
